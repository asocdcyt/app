          <div class="col-auto">
            <div class="avatar avatar-xl position-relative">
            <img src="/img/team-4.jpg" alt="profile_image" class="w-100 border-radius-lg shadow-sm">
            </div>
          </div>
          <div class="col-auto my-auto">
            <div class="h-100">
            <h5 class="mb-1">
            {{ name }} {{ lastname }}
            </h5>
