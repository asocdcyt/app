                  </li>
                </ul>
              </div>
          </div>
        </div>
      </div>

          {{/each}}
    </div>
    <div class="container-fluid py-4">
      {{#if success_msg }}
            <div class="alert alert-success alert-dismissible fade show" role="alert">
                <span class="alert-icon text-white"><i class="ni ni-like-2"></i></span>
                <span class="alert-text text-white"><strong>¡Enhorabuena! -</strong> {{ success_msg }}</span>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            {{/if}}
            {{#if error}}
              <div class="alert alert-danger alert-dismissible fade show" role="alert">
                  <span class="alert-icon text-white"><i class="ni ni-support-16"></i></span>
                  <span class="alert-text text-white"><strong>¡Oh no!</strong> {{error}}</span>
                  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                  </button>
              </div>
            {{/if}}
            {{#each errors }}
              <div class="alert alert-danger alert-dismissible fade show" role="alert">
                  <span class="alert-icon text-white"><i class="ni ni-support-16"></i></span>
                  <span class="alert-text text-white"><strong>¡Oh no!</strong> {{text}}</span>
                  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                  </button>
              </div>
            {{/each}}
            {{#if error_msg }}
              <div class="alert alert-danger alert-dismissible fade show" role="alert">
                  <span class="alert-icon text-white"><i class="ni ni-support-16"></i></span>
                  <span class="alert-text text-white"><strong>¡Oh no!</strong> {{error_msg}}</span>
                  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
                      <span aria-hidden="true">&times;</span>
                  </button>
              </div>
            {{/if}}
      <div class="row">
        <div class="col-12 col-xl-12">
          <div class="card h-100">
            <div class="card-body p-3">
              {{#each users}}
              <form>
                <input type="hidden" name="_method" value="PUT">
                <h6 class="heading-small text-muted mb-4">Informacion del Egresado</h6>
                <div class="pl-lg-4">
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Número de Cédula de Identidad</label>
                        <input type="text" disabled class="form-control" maxlength="100" value="V- {{idcard}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Correo Electrónico</label>
                        <input type="email"  disabled class="form-control" value="{{email}}">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Nombres</label>
                        <input type="text" disabled class="form-control" placeholder="Nombres" value="{{name}}">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Apellidos</label>
                        <input type="text" disabled class="form-control" placeholder="Apellidos" value="{{lastname}}">
                      </div>
                    </div>
                    
                  </div>
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Título Universitario</label>
                        <input type="text" disabled class="form-control" value="{{collegedegree}}">
                      </div>
                    </div>
                    <div class="col-lg-3">
                      <div class="form-group">
                        <label class="form-control-label">Año de Graduación</label>
                        <input type="text" disabled class="form-control" value="{{lastyear}}">
                      </div>
                    </div>
                    <div class="col-lg-3">
                      <div class="form-group">
                        <label class="form-control-label">Número de Promoción</label>
                        <input type="text" disabled class="form-control" value="{{promotion}}">
                      </div>
                    </div>
                    
                  </div>
                </div>
                <hr class="my-4" />
                <!-- Address -->
                <h6 class="heading-small text-muted mb-4">Información de Contacto</h6>
                <div class="pl-lg-4">
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Número de WhatsApp</label>
                        <input type="number" disabled class="form-control" maxlength="100" value="{{ws}}" placeholder="Número de WhatsApp">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label">Número de Teléfono</label>
                        <input type="number"  disabled class="form-control" value="{{phone}}" placeholder="Número de Teléfono">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-12">
                      <div class="form-group">
                        <label class="form-control-label">Dirección</label>
                        <input class="form-control" disabled placeholder="Direccion fiscal" type="text" value="{{address}}">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-lg-4">
                      <div class="form-group">
                        <label class="form-control-label">Ciudad</label>
                        <input type="text" disabled class="form-control" placeholder="Ciudad" value="{{city}}">
                      </div>
                    </div>
                    <div class="col-lg-4">
                      <div class="form-group">
                        <label class="form-control-label">País</label>
                        <input type="text" disabled class="form-control" placeholder="País..." value="{{country}}">
                      </div>
                    </div>
                    <div class="col-lg-4">
                      <div class="form-group">
                        <label class="form-control-label">Código Postal</label>
                        <input type="number" disabled class="form-control" placeholder="Código Postal" value="{{postalcode}}">
                      </div>
                    </div>
                  </div>
                </div>
                <hr class="my-4" />
                <!-- Description -->
                <h6 class="heading-small text-muted mb-4">Escriba algo sobre usted.</h6>
                <div class="pl-lg-4">
                  <div class="form-group">
                    <label class="form-control-label">Sobre mí</label>
                    <textarea rows="4" class="form-control" disabled placeholder="Algunas palabras sobre ti..">{{description}}</textarea>
                  </div>
                </div>
              </form>
                {{/each}}
            </div>
          </div>
        </div>
      </div>
    </div>
    
